# splits string into a list to parse
connected_papers = s.lower()
connected_paper_list = connected_papers.split("\n\n")
cs = list(set(connected_paper_list))

# creates a dictionary to house all of the connected papers 
connected_papersdict = {}

for paper in connected_paper_list:
  if paper in connected_papersdict:
    connected_papersdict[paper] = connected_papersdict[paper] + 1
  else:
    connected_papersdict[paper] = 1

for paper in connected_paper_list:
  if connected_papersdict[paper] > 1:
    print("Paper is {} \n and the number of duplicates is {}".format(paper, str(connected_papersdict[paper])))
    print("\n")
  else:
    print("null")

# for a shorter way, you can also do:
  #cs = list(set(connected_paper_list))
  #print(cs)
# it will print the list without the duplicate values

